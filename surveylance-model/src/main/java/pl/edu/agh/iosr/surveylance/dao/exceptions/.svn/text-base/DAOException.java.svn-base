package pl.edu.agh.iosr.surveylance.dao.exceptions;

/**
 * This exception is thrown by DAO classes.
 * 
 * @author kornel
 */
public class DAOException extends RuntimeException {

	private static final long serialVersionUID = 1L;

	/**
	 * Public constructor.
	 */
	public DAOException() {
		super();
	}

	/**
	 * Public constructor.
	 * 
	 * @param arg0
	 *            error message
	 */
	public DAOException(String arg0) {
		super(arg0);
	}

	/**
	 * Public constructor.
	 * 
	 * @param arg0
	 *            error message
	 * @param arg1
	 *            exception
	 */
	public DAOException(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

	/**
	 * Public constructor.
	 * 
	 * @param arg0
	 *            exception
	 */
	public DAOException(Throwable arg0) {
		super(arg0);
	}

}
